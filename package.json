{
  "name": "rxjs-playground",
  "version": "1.0.0",
  "description": "A playground for all things RxJS",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify src/index.js -o dist/bundle.js",
    "watch": "watchify src/index.js -o dist/bundle.js -v"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hghazni/RxJS-Playground.git"
  },
  "keywords": [
    "rxjs"
  ],
  "author": "Harry Ghazni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hghazni/RxJS-Playground/issues"
  },
  "homepage": "https://github.com/hghazni/RxJS-Playground#readme",
  "dependencies": {
    "browserify": "^16.5.0",
    "rxjs": "^6.5.4",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0"
  }
}
